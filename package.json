{
  "name": "article-auto-submit",
  "version": "2.0.0",
  "description": "Dynamic multi-website article submission automation using Selenium",
  "main": "src/dynamic-article-submitter.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node src/multi-website-runner.js",
    "start:single": "node scripts/single-website.js",
    "start:multi": "node scripts/multi-website.js",
    "validate:config": "node scripts/validate-config.js",
    "create:config": "node scripts/create-config.js",
    "test:config": "node scripts/test-config.js",
    "clean:screenshots": "rm -rf screenshots/*",
    "clean:reports": "rm -rf reports/*",
    "clean:logs": "rm -rf logs/*",
    "clean:all": "npm run clean:screenshots && npm run clean:reports && npm run clean:logs",
    "setup": "node scripts/setup.js",
    "dev": "node --inspect src/multi-website-runner.js"
  },
  "keywords": [
    "selenium",
    "automation",
    "article",
    "submission",
    "cms",
    "wordpress",
    "multi-website",
    "dynamic",
    "web-scraping"
  ],
  "author": "Your Name",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.2.0",
    "inquirer": "^9.2.0",
    "moment": "^2.30.1",
    "ora": "^5.4.1",
    "puppeteer": "^24.12.1",
    "selenium-webdriver": "^4.35.0"
  },
  "devDependencies": {
    "ajv-formats": "^2.1.1",
    "chromedriver": "^138.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "bugs": {
    "url": "your-repository-url/issues"
  },
  "homepage": "your-repository-url#readme",
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
